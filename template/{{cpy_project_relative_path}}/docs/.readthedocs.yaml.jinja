# Read the Docs configuration file for {{ cpy_project_name }}
# See https://docs.readthedocs.io/en/stable/config-file/v2.html

version: 2

build:
  os: ubuntu-22.04
  tools:
    python: "3.11"
  apt_packages:
    - doxygen
  commands:
    - pip install -r {{ cpy_project_relative_path }}/docs/requirements.txt
    - cd {{ cpy_project_relative_path }} && mkdocs build --site-dir $READTHEDOCS_OUTPUT/html
